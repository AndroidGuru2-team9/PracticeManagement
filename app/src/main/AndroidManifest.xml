<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.practicemanagement">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PracticeManagement"
        android:usesCleartextTraffic="true">
        <activity android:name=".ReviseCertificate"></activity>
        <activity android:name=".PrizeList" />
        <activity android:name=".CertificateList" />
        <activity android:name=".WritePrize" />
        <activity android:name=".CertificateView" />
        <activity android:name=".WriteCertificate" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>